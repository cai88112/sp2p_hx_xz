<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="homeInfoSetting" extends="struts-default"
		namespace="/">
		<global-results>
			<result name="error">/500.jsp</result>
		</global-results>
		<global-exception-mappings>
			<exception-mapping result="error" exception="java.lang.Exception"></exception-mapping>
		</global-exception-mappings>
		<action name="homeInfoSettingInit" class="homeInfoSettingAction"
			method="homeInfoSettingInit">
			<result name="success">/WEB-INF/page/front/wdzh-grsz-index.jsp</result>
		</action>


	</package>
</struts>