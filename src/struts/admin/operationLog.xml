<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="adminRecordInfo" extends="admin-default"
		namespace="/admin">
		<global-results>
			<result name="error">/500.jsp</result>
		</global-results>
		<global-exception-mappings>
			<exception-mapping result="error" exception="java.lang.Exception"></exception-mapping>
		</global-exception-mappings>
		<action name="queryRecordInit" class="operationLogAction"
			method="queryRecordInit">
			<result name="success">/WEB-INF/page/admin/admin_log_index.jsp
			</result>
		</action>

		<action name="queryRecordInfo" class="operationLogAction"
			method="queryRecordInfo">
			<result name="success">/WEB-INF/page/admin/admin_log_index_info.jsp
			</result>
		</action>
	</package>
</struts>
